  <!-- Bootstrap 5.3.x JavaScript Bundle (includes Popper) -->
  <!-- Educational: Placed at end of body for better page load performance -->
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"
          integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL"
          crossorigin="anonymous"></script>

  <!-- Apache ECharts 5.x for Data Visualization -->
  <!-- Educational: Only load on dashboard page to reduce bandwidth -->
  <% if (typeof needsEcharts !== 'undefined' && needsEcharts) { %>
  <script src="https://cdn.jsdelivr.net/npm/echarts@5.4.3/dist/echarts.min.js"
          integrity="sha384-J0AvK3qzOqJJJ9FkiuqvJ8iDc7dJ5bH4t5sHAJ+DPvJq5gF5L5eoRLhI5xQ5h5gK"
          crossorigin="anonymous"></script>
  <% } %>

  <!-- Page-specific JavaScript -->
  <% if (typeof scripts !== 'undefined' && scripts.length > 0) { %>
    <% scripts.forEach(function(script) { %>
      <script src="<%= script %>"></script>
    <% }); %>
  <% } %>

</body>
</html>
